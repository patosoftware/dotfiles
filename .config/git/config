[diff]
tool = code

[difftool "code"]
cmd = code --wait --diff $LOCAL $REMOTE

[init]
defaultBranch = main

[merge]
tool = code

[mergetool "code"]
cmd = code --wait --merge $REMOTE $LOCAL $BASE $MERGED

[pull]
rebase = false

